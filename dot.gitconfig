[user]
	email = vieuxtech@gmail.com
	name = Sam Roberts
[clean]
	requireForce = false
[alias]
	cl = clean -x
	st = status
	ci = commit
	br = branch
	b = branch -av
	co = checkout
	df = diff --word-diff
	lg = log --stat --decorate
	pick = cherry-pick
	tg = show-ref --tags
[core]
	excludesfile = ~/.gitignore
	editor = /usr/local/bin/vim
[help]
	autocorrect = -1
[push]
	default = tracking
[color]
	ui = true
